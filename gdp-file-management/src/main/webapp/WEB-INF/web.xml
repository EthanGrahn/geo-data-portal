<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>gdp-file-management</display-name>
    <servlet>
        <description>
            Receives files from client in a multi-part form upload
        </description>
        <display-name>UploadFilesServlet</display-name>
        <servlet-name>UploadFilesServlet</servlet-name>
        <servlet-class>
            gov.usgs.cida.gdp.filemanagement.servlet.UploadFilesServlet
        </servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UploadFilesServlet</servlet-name>
        <url-pattern>/UploadFilesServlet</url-pattern>
    </servlet-mapping>

</web-app>
