<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>gdp-data-access-webapp</display-name>
    <servlet>
        <description>
            ...
        </description>
        <display-name>WCSServlet</display-name>
        <servlet-name>WCSServlet</servlet-name>
        <servlet-class>
            gov.usgs.cida.gdp.dataaccess.servlet.WCSServlet
        </servlet-class>
    </servlet>

    <servlet>
        <servlet-name>WPSFileUpload</servlet-name>
        <servlet-class>gov.usgs.cida.gdp.dataaccess.servlet.WPSFileUpload</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>WPSFileUpload</servlet-name>
        <url-pattern>/WPSFileUpload</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>WCSServlet</servlet-name>
        <url-pattern>/WCSServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <display-name>THREDDSCheckServlet</display-name>
        <servlet-name>THREDDSCheckServlet</servlet-name>
        <servlet-class>
            gov.usgs.cida.gdp.dataaccess.servlet.THREDDSCheckServlet
        </servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>THREDDSCheckServlet</servlet-name>
        <url-pattern>/THREDDSCheckServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <display-name>THREDDSServlet</display-name>
        <servlet-name>THREDDSServlet</servlet-name>
        <servlet-class>
            gov.usgs.cida.gdp.dataaccess.servlet.THREDDSServlet
        </servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>THREDDSServlet</servlet-name>
        <url-pattern>/THREDDSServlet</url-pattern>
    </servlet-mapping>
    
</web-app>
