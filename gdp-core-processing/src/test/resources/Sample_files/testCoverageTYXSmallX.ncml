<?xml version="1.0" encoding="UTF-8"?>
<netcdf xmlns="http://www.unidata.ucar.edu/namespaces/netcdf/ncml-2.2">
  <dimension name="time" length="3" isUnlimited="true" />
  <dimension name="lat" length="3" />
  <dimension name="lon" length="2" />
  <attribute name="title" value="testGridCellCoverage Data" />
  <attribute name="Conventions" value="CF-1.4" />
  <variable name="rh" shape="time lat lon" type="int">
    <attribute name="long_name" value="relative humidity" />
    <attribute name="units" value="percent" />
    <values start="10" increment="5" />
  </variable>
  <variable name="lat" shape="lat" type="float">
    <attribute name="units" value="degrees_north" />
    <values>40 45 50</values>
  </variable>
  <variable name="lon" shape="lon" type="float">
    <attribute name="units" value="degrees_east" />
    <values>-96 -92</values>
  </variable>
  <variable name="time" shape="time" type="int">
    <attribute name="units" value="hours" />
    <values start="1" increment="1" />
  </variable>
</netcdf>